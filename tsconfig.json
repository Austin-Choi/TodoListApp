{
    "compilerOptions": {
        "target": "ES5",
        "lib": ["DOM","DOM.Iterable","ESNext"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true ,
        "allowImportingTsExtensions": true,
        "noEmit": true
    },
    "include": ["src"]
}